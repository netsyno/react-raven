{
  "name": "react-raven",
  "version": "1.0.4",
  "description": "A simple React component for Sentry integration.",
  "main": "dist/Raven.js",
  "module": "es/Raven.js",
  "scripts": {
    "build": "npm run build:commonjs && npm run build:es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs NODE_ENV=production babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=production babel src --out-dir es",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khakulov/react-raven.git"
  },
  "keywords": [
    "react",
    "raven",
    "reactjs",
    "ravenjs",
    "sentry",
    "getsentry"
  ],
  "author": "Azim Khakulov <azim@khakulov.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/khakulov/react-raven/issues"
  },
  "homepage": "https://github.com/khakulov/react-raven#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3"
  },
  "peerDependencies": {
    "prop-types": ">15.5.0",
    "raven-js": "^3.14.2",
    "react": ">15.5.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "files": [
    "README.md",
    "LICENCSE",
    "dist",
    "es"
  ]
}
